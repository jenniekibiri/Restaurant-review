{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "rules": {
    "no-unused-vars": "off",
    "eqeqeq": "off",
    "no-console":"off",
    "quotes": ["warn","double"]
      }
}
